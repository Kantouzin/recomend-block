{
    "manifest_version": 2,
    "name": "Recommendation Blocker",
    "description": "\"他の人はこちらも検索\"をBlockします",
    "version": "0.3",
    "icons": {
        "128": "img/icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/search?*",
                "https://www.google.co.jp/search?*"
            ],
            "css": ["css/style.css"],
            "run_at": "document_end"
        }
    ]
}
